{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/getList", "dest": "/api/getList.js" },
    { "src": "/api/updateTasks", "dest": "/api/updateTasks.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}