{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/getList", "dest": "/api/getList.js" },
    { "src": "/api/updateTasks", "dest": "/api/updateTasks.js" },
    { "src": "/api/processTask", "dest": "/api/processTask.js" },
    { "src": "/api/getTasks", "dest": "/api/getTasks.js" },
    { "src": "/api/getListStats", "dest": "/api/getListStats.js" },
    { "src": "/api/fetchListTasks", "dest": "/api/fetchListTasks.js" },
    { "src": "/api/archiveBatch", "dest": "/api/archiveBatch.js" },
    { "src": "/api/clickup", "status": 404, "dest": "/api/getList.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}