{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { 
      "src": "/api/clickup", 
      "dest": "/api/clickup.js",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}